    "%matplotlib inline\n",
    "def create_plot(plot_df, threshold=178.5, oligo_name=\"\", position=60, figsize=(6.3, 1.165)):\n",
    "    \"\"\"Create boxen plot with density-scaled confusion matrix.\"\"\"\n",
    "    \n",
    "    # Create figure with 1 row, 2 columns for side-by-side layout\n",
    "    fig, (ax_box, ax_cm) = plt.subplots(1, 2, figsize=figsize, \n",
    "                                         gridspec_kw={'width_ratios': [2, 1]})\n",
    "    \n",
    "    # Adjust spacing for the small height\n",
    "    plt.subplots_adjust(wspace=0.3)\n",
    "    \n",
    "    # BOXEN PLOT\n",
    "    positions = sorted(plot_df['ref_pos'].unique())\n",
    "    \n",
    "    sns.boxenplot(\n",
    "        x='ref_pos', y='ML_score', hue='condition', data=plot_df,\n",
    "        palette='Set1',\n",
    "        ax=ax_box, dodge=True, k_depth=10,\n",
    "        order=positions\n",
    "    )\n",
    "    \n",
    "    ax_box.axhline(y=threshold, color='red', linestyle='--', linewidth=0.5, alpha=0.7)\n",
    "    \n",
    "    # Smaller font sizes for small figure height\n",
    "    ax_box.set_title(f'{oligo_name}', fontsize=10, pad=2)\n",
    "    ax_box.set_xlabel('Reference Position', fontsize=9)\n",
    "    ax_box.set_ylabel('ML Score', fontsize=9)\n",
    "    ax_box.tick_params(axis='both', which='major', labelsize=9)\n",
    "    \n",
    "    # Adjust legend for small height\n",
    "    ax_box.legend(title='Condition', loc='upper left', fontsize=9, \n",
    "                  title_fontsize=10, frameon=True, fancybox=False, \n",
    "                  borderaxespad=0.1, handlelength=0.5, handletextpad=0.3)\n",
    "    \n",
    "    # CONFUSION MATRIX\n",
    "    metrics = calculate_confusion_metrics_position(plot_df, position, threshold)\n",
    "    \n",
    "    if metrics:\n",
    "        # Compact labels for small size\n",
    "        cm_labels = np.array([\n",
    "            [f\"{metrics['tp']:,}\\n({metrics['tp_pct']:.1f}%)\", \n",
    "             f\"{metrics['fn']:,}\\n({metrics['fn_pct']:.1f}%)\"],\n",
    "            [f\"{metrics['fp']:,}\\n({metrics['fp_pct']:.1f}%)\", \n",
    "             f\"{metrics['tn']:,}\\n({metrics['tn_pct']:.1f}%)\"]\n",
    "        ])\n",
    "        \n",
    "        cm_values = np.array([[metrics['tp'], metrics['fn']], \n",
    "                             [metrics['fp'], metrics['tn']]])\n",
    "        \n",
    "        sns.heatmap(cm_values, annot=cm_labels, fmt='', cmap='Blues',\n",
    "                   xticklabels=['Predicted\\nModified', 'Predicted\\nCanonical'],\n",
    "                   yticklabels=['Modified', 'Canonical'],\n",
    "                   cbar=False, ax=ax_cm,\n",
    "                   linewidths=0.5, linecolor='black',\n",
    "                   annot_kws={'size': 10})\n",
    "        \n",
    "        ax_cm.set_ylabel('Actual', fontsize=10)\n",
    "        ax_cm.set_xlabel('', fontsize=10)\n",
    "        ax_cm.set_title(f'Position {position} Confusion Matrix', fontsize=10, pad=2)\n",
    "        ax_cm.tick_params(axis='both', which='major', labelsize=9)\n",
    "    \n",
    "    plt.tight_layout(pad=0.2)\n",
    "    return fig\n",
    "\n",
    "# Create the plot with custom size\n",
    "plot_df['ref_pos_str'] = plot_df['ref_pos'].astype(str)\n",
